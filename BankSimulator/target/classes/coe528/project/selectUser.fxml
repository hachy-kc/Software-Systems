<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coe528.project.selectUserController">
   <children>
       <Label text="Welcome, please pick a user type:" />
       <Button fx:id = "managerSwitch" text = "Manager" onAction ="#switchToManager" /> 
       <Button fx:id="customerSwitch" text = "Customer" onAction ="#switchToCustomer"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
